	<queryString>
		<![CDATA[SELECT empleado.nombre AS nombre,MAX(fechadeventa)AS fecha, SUM(cantidad) AS cantidad 
FROM ventas INNER JOIN empleado ON ventas.empleado_id = empleado.id
WHERE fechadeventa BETWEEN '$P!{fechaIni}' AND '$P!{fechaFin}'
GROUP BY nombre  ORDER BY cantidad DESC;]]>
