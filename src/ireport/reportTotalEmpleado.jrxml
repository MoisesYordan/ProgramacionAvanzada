	<subDataset name="cone2">
		<parameter name="fechaIni" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="fechaFin" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT empleado.nombre AS nombre,MAX(fechadeventa)AS fecha, SUM(total) AS total 
FROM ventas INNER JOIN empleado ON ventas.empleado_id = empleado.id
WHERE fechadeventa BETWEEN '$P!{fechaIni}' AND '$P!{fechaFin}'
GROUP BY nombre  ORDER BY total DESC;]]>
		</queryString>
		<field name="nombre" class="java.lang.String"/>
		<field name="fecha" class="java.lang.String"/>
		<field name="total" class="java.lang.Long"/>
	</subDataset>
